{
  "name": "artichoke-site",
  "version": "0.1.0",
  "private": true,
  "description": "Artichoke Site",
  "keywords": [
    "programming language",
    "scripting",
    "ruby",
    "rust",
    "wasm"
  ],
  "homepage": "https://www.artichoke-ruby.org",
  "bugs": "https://github.com/artichoke/artichoke.github.io/issues",
  "license": "MIT",
  "author": {
    "name": "Ryan Lopopolo",
    "email": "rjl@hyperbo.la",
    "url": "https://hyperbo.la"
  },
  "contributors": [
    {
      "name": "Ryan Lopopolo",
      "email": "rjl@hyperbo.la",
      "url": "https://hyperbo.la"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/artichoke/artichoke.github.io.git"
  },
  "devDependencies": {
    "@artichoke/logo": "git://github.com/artichoke/logo#master",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.5.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "clang-format": "^1.2.4",
    "css-loader": "^3.1.0",
    "eslint": "^5.11.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.0",
    "file-loader": "^4.1.0",
    "gh-pages": "^2.1.1",
    "html-webpack-inline-source-plugin": "^0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "package-json-validator": "^0.6.3",
    "path": "^0.12.7",
    "pkg-dir-cli": "^2.1.0",
    "prettier": "^1.15.3",
    "prettier-eslint": "^9.0.0",
    "raw-loader": "^3.1.0",
    "script-loader": "^0.7.2",
    "style-loader": "^0.23.1",
    "svg-url-loader": "^3.0.0",
    "svgo": "^1.3.0",
    "svgo-loader": "^2.2.1",
    "terser-webpack-plugin": "^1.3.0",
    "uglify-loader": "^3.0.0",
    "url-loader": "^2.1.0",
    "webassembly-loader": "^1.1.0",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "parser": "babel-eslint",
    "plugins": [
      "html",
      "import",
      "prettier"
    ],
    "extends": [
      "airbnb",
      "prettier",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "rules": {
      "prettier/prettier": "error",
      "react/jsx-closing-bracket-location": "off",
      "react/jsx-closing-tag-location": "off",
      "react/jsx-curly-spacing": "off",
      "react/jsx-equals-spacing": "off",
      "react/jsx-first-prop-new-line": "off",
      "react/jsx-indent": "off",
      "react/jsx-indent-props": "off",
      "react/jsx-max-props-per-line": "off",
      "react/jsx-one-expression-per-line": "off",
      "react/jsx-props-no-multi-spaces": "off",
      "react/jsx-tag-spacing": "off",
      "react/jsx-wrap-multilines": "off",
      "react/self-closing-comp": "off"
    },
    "settings": {
      "import/resolver": "webpack"
    }
  },
  "scripts": {
    "lint": "./scripts/lint.sh",
    "loc": "loc --exclude vendor",
    "serve": "webpack-dev-server --mode development --content-base target/webpack/debug/ --open"
  },
  "dependencies": {
    "ace-builds": "^1.4.5",
    "bootstrap": "^4.3.1",
    "jquery": "^3.4.1",
    "popper.js": "^1.15.0"
  }
}
